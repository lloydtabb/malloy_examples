source: usa_spending is table('duckdb:/usr/local/google/home/ltabb/s/malloy_examples/usaspending.gov/FY2022_All_Contracts_Full_20220510_*.parquet') {
  measure: award_count is count()
  measure: total_award is current_total_value_of_award.sum()

  query: by_agency is {
    group_by: funding_agency_name
    aggregate: award_count, total_award
    nest: sub_agency is {
      group_by: funding_sub_agency_name
      aggregate: award_count, total_award
    }
  }
}